/* pages/duobaohome/duobaohome.wxss */
.topbody{
  position: relative;
  height: 900rpx;
}
.hometop{
  height: 510rpx;
  position: relative;
  
}
.homeimagetop{
  width: 100%;
  background-color: white;
  height: 450rpx;
}
.hometopcontent{
  left: 20rpx;
  right: 20rpx;
  height: 180rpx;
  background-color: white;
  border-radius: 10rpx;
  overflow: hidden;
  bottom: 0rpx;
  position: absolute;
  box-shadow: 3px 3px 3px 3px #e6e2e2;
}
.homecententbg{
  background-color: white;
  width: 100%;
  height: 350rpx;
  top: 435rpx;
  z-index: -1;
  position: absolute;
  overflow: hidden;
  
}
.cenbgimage{
  width: 100%;
}
.cententbuttonview{
  width: 25%;
  height: 100%;
  background-color: white;
  display: inline-flex;
  flex-direction: column;
  align-items: center;
  font-size: 27rpx;
  
}
.cententbuttonview text{
  margin-top: 12rpx;
}
.cenbuttonimage{
  margin-top: 30rpx;
  margin-bottom: 0rpx;
  width: 50%;
}
.cengbgbgview{
  position: relative;
  width: 140*22rpx;
  top: 290rpx;
  height: 110rpx;
  display: flex;
  white-space: nowrap;
  background-color: white;
}
.cenitemview{
  width: 140rpx;
  height: 50rpx;
  border-radius: 15rpx;
  overflow: hidden;
  background-color: white;
  text-align: center;
  line-height: 50rpx;
  margin-top: 25rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
  display: inline-block;
  color: black;
  border-color: black;
  border-width: 2rpx;
  border-style: dotted;
  font-size: 28rpx;
}
.cenitemview.taphi{
  color: #FF6347;
}
.zuixinjiexiao{
  width: 50%;
  position: absolute;
  left: 20rpx;
  top: 100rpx;
  color: gray;
  font-size: 28rpx;
}
.zuixinjiexiaoall{
  width: 50%;
  position: absolute;
  right: 20rpx;
  top: 100rpx;
  color: gray;
  font-size: 28rpx;
  text-align: right;
}
.zuixinjiexiaoview{
  position: absolute;
  left: 20rpx;
  right: 20rpx;
  bottom: 0rpx;
  height: 195rpx;
  display: flex;
}
.newbgview{
  flex: 1;
  display: flex;
  flex-flow: column;
  align-items: center;

}
.newbgview image{
  width: 130rpx;
  height: 130rpx;
  margin-top: 10rpx;
  
}
.newbgview text{
    margin-top: 15rpx;
    border-radius: 7rpx;
    padding-right: 20rpx;
    padding-left: 20rpx;
    background-color:#FF6347;
    font-size: 25rpx;
    color: white;
}

.bottomitem{
  position: relative;
  margin-left: 20rpx;
  margin-right: 20rpx;
  column-count: 2;
  column-gap: 10rpx;
  margin-bottom: 20rpx;
  background-color: rgba(56, 56, 55, 0.03);
}
.listitemview{
  width: 100%;
  margin-bottom: 10rpx;
  background-color: white;
  overflow: hidden;
  display: inline-block;
}
.listitemview image{
  width: 100%;
  margin: 5rpx;
}
.listtext{
  display: block;
  margin-bottom: 5rpx;
  margin-left: 15rpx;
  margin-right: 15rpx;
  font-size: 27rpx;
}
.itemprice{
  color: #FF6347;
  font-size: 35rpx;
  margin-left: 15rpx;
  margin-right: 15rpx;
}
.itemjindu{
  display: block;
  margin-top: 10rpx;
  font-size: 25rpx;
  margin-left: 15rpx;
  margin-right: 15rpx;
}
.itemjindutiao{
  margin-left: 15rpx;
  margin-right: 15rpx;
  margin-bottom: 10rpx;
  margin-top: 5rpx;
}
.bottommore{
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  background-color: white;
  font-size: 28rpx;
  color: rgba(56, 56, 55, 1);
}
.bottommore.more{
  background-color: rgba(56, 56, 55, 0.03);
  color: black;
}